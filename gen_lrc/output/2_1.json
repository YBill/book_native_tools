{
 "fragments": [
  {
   "begin": "0.000",
   "children": [],
   "end": "2.280",
   "id": "f000001",
   "language": "ind",
   "lines": [
    "Mengubah Pagi,"
   ]
  },
  {
   "begin": "2.280",
   "children": [],
   "end": "5.680",
   "id": "f000002",
   "language": "ind",
   "lines": [
    "Mengubah Hidup: Mulai Sekarang Juga!"
   ]
  },
  {
   "begin": "5.680",
   "children": [],
   "end": "9.120",
   "id": "f000003",
   "language": "ind",
   "lines": [
    "Pagi hari adalah waktu yang penuh potensi."
   ]
  },
  {
   "begin": "9.120",
   "children": [],
   "end": "15.680",
   "id": "f000004",
   "language": "ind",
   "lines": [
    "Bagaimana kita memulai hari kita bisa menentukan bagaimana sisa hari akan berjalan."
   ]
  },
  {
   "begin": "15.680",
   "children": [],
   "end": "18.160",
   "id": "f000005",
   "language": "ind",
   "lines": [
    "Jika kita bisa mengubah rutinitas pagi kita,"
   ]
  },
  {
   "begin": "18.160",
   "children": [],
   "end": "20.520",
   "id": "f000006",
   "language": "ind",
   "lines": [
    "kita juga bisa mengubah hidup kita."
   ]
  },
  {
   "begin": "20.520",
   "children": [],
   "end": "22.040",
   "id": "f000007",
   "language": "ind",
   "lines": [
    "Ini bukan sekadar omong kosong,"
   ]
  },
  {
   "begin": "22.040",
   "children": [],
   "end": "22.640",
   "id": "f000008",
   "language": "ind",
   "lines": [
    "lho."
   ]
  },
  {
   "begin": "22.640",
   "children": [],
   "end": "30.280",
   "id": "f000009",
   "language": "ind",
   "lines": [
    "Ada banyak contoh nyata yang membuktikan bahwa rutinitas pagi yang kuat dapat membawa perubahan luar biasa dalam kehidupan seseorang."
   ]
  },
  {
   "begin": "30.280",
   "children": [],
   "end": "33.400",
   "id": "f000010",
   "language": "ind",
   "lines": [
    "Coba ingat kembali orang-orang sukses yang kita kagumi."
   ]
  },
  {
   "begin": "33.400",
   "children": [],
   "end": "37.640",
   "id": "f000011",
   "language": "ind",
   "lines": [
    "Banyak dari mereka yang memiliki rutinitas pagi yang disiplin dan penuh makna."
   ]
  },
  {
   "begin": "37.640",
   "children": [],
   "end": "42.400",
   "id": "f000012",
   "language": "ind",
   "lines": [
    "Mulai dari Tony Robbins yang bangun pagi-pagi untuk berolahraga dan melakukan meditasi,"
   ]
  },
  {
   "begin": "42.400",
   "children": [],
   "end": "46.800",
   "id": "f000013",
   "language": "ind",
   "lines": [
    "hingga Oprah Winfrey yang memulai harinya dengan menulis jurnal dan berdoa."
   ]
  },
  {
   "begin": "46.800",
   "children": [],
   "end": "54.280",
   "id": "f000014",
   "language": "ind",
   "lines": [
    "Mereka semua tahu satu hal: bagaimana kita memulai hari kita sangat berpengaruh terhadap energi dan fokus kita sepanjang hari."
   ]
  },
  {
   "begin": "54.280",
   "children": [],
   "end": "56.800",
   "id": "f000015",
   "language": "ind",
   "lines": [
    "Ada banyak penelitian yang mendukung hal ini."
   ]
  },
  {
   "begin": "56.800",
   "children": [],
   "end": "60.880",
   "id": "f000016",
   "language": "ind",
   "lines": [
    "Salah satu penelitian yang menarik dilakukan oleh University of London,"
   ]
  },
  {
   "begin": "60.880",
   "children": [],
   "end": "71.520",
   "id": "f000017",
   "language": "ind",
   "lines": [
    "yang menemukan bahwa orang yang memiliki rutinitas pagi yang terstruktur cenderung memiliki produktivitas yang lebih tinggi dan lebih sedikit stres dibandingkan mereka yang tidak memiliki rutinitas."
   ]
  },
  {
   "begin": "71.520",
   "children": [],
   "end": "77.480",
   "id": "f000018",
   "language": "ind",
   "lines": [
    "Ini menunjukkan bahwa kebiasaan kita di pagi hari memengaruhi kesejahteraan kita secara keseluruhan."
   ]
  },
  {
   "begin": "77.480",
   "children": [],
   "end": "80.280",
   "id": "f000019",
   "language": "ind",
   "lines": [
    "Rutinitas yang baik tidak hanya mengatur waktu kita,"
   ]
  },
  {
   "begin": "80.280",
   "children": [],
   "end": "84.520",
   "id": "f000020",
   "language": "ind",
   "lines": [
    "tetapi juga membantu kita fokus pada hal-hal yang paling penting."
   ]
  },
  {
   "begin": "84.520",
   "children": [],
   "end": "86.800",
   "id": "f000021",
   "language": "ind",
   "lines": [
    "Bukan hanya penelitian yang mengatakan ini,"
   ]
  },
  {
   "begin": "86.800",
   "children": [],
   "end": "92.760",
   "id": "f000022",
   "language": "ind",
   "lines": [
    "cerita sukses dari orang-orang yang telah mengubah hidup mereka dengan rutinitas pagi juga sangat inspiratif."
   ]
  },
  {
   "begin": "92.760",
   "children": [],
   "end": "93.880",
   "id": "f000023",
   "language": "ind",
   "lines": [
    "Seperti misalnya,"
   ]
  },
  {
   "begin": "93.880",
   "children": [],
   "end": "95.960",
   "id": "f000024",
   "language": "ind",
   "lines": [
    "hal yang dilakukan oleh Hal Elrod sendiri,"
   ]
  },
  {
   "begin": "95.960",
   "children": [],
   "end": "97.280",
   "id": "f000025",
   "language": "ind",
   "lines": [
    "penulis buku ini."
   ]
  },
  {
   "begin": "97.280",
   "children": [],
   "end": "99.120",
   "id": "f000026",
   "language": "ind",
   "lines": [
    "Sebelum menemukan \"The Miracle Morning\","
   ]
  },
  {
   "begin": "99.120",
   "children": [],
   "end": "101.680",
   "id": "f000027",
   "language": "ind",
   "lines": [
    "dia adalah seseorang yang mengalami banyak kegagalan,"
   ]
  },
  {
   "begin": "101.680",
   "children": [],
   "end": "105.040",
   "id": "f000028",
   "language": "ind",
   "lines": [
    "bahkan kehilangan nyawa dalam sebuah kecelakaan mobil."
   ]
  },
  {
   "begin": "105.040",
   "children": [],
   "end": "105.600",
   "id": "f000029",
   "language": "ind",
   "lines": [
    "Namun,"
   ]
  },
  {
   "begin": "105.600",
   "children": [],
   "end": "109.520",
   "id": "f000030",
   "language": "ind",
   "lines": [
    "setelah dia mulai menerapkan rutinitas pagi yang penuh dengan aktivitas positif,"
   ]
  },
  {
   "begin": "109.520",
   "children": [],
   "end": "110.760",
   "id": "f000031",
   "language": "ind",
   "lines": [
    "hidupnya berubah."
   ]
  },
  {
   "begin": "110.760",
   "children": [],
   "end": "112.600",
   "id": "f000032",
   "language": "ind",
   "lines": [
    "Dia mendapatkan kesehatan yang lebih baik,"
   ]
  },
  {
   "begin": "112.600",
   "children": [],
   "end": "114.680",
   "id": "f000033",
   "language": "ind",
   "lines": [
    "produktivitas yang meningkat,"
   ]
  },
  {
   "begin": "114.680",
   "children": [],
   "end": "118.440",
   "id": "f000034",
   "language": "ind",
   "lines": [
    "dan mencapai banyak tujuan yang sebelumnya tampak mustahil."
   ]
  },
  {
   "begin": "118.440",
   "children": [],
   "end": "118.920",
   "id": "f000035",
   "language": "ind",
   "lines": [
    "Jadi,"
   ]
  },
  {
   "begin": "118.920",
   "children": [],
   "end": "123.400",
   "id": "f000036",
   "language": "ind",
   "lines": [
    "bagaimana kita memulai hari kita sangat berpengaruh pada kualitas hidup kita."
   ]
  },
  {
   "begin": "123.400",
   "children": [],
   "end": "126.040",
   "id": "f000037",
   "language": "ind",
   "lines": [
    "Dengan memulai pagi hari dengan niat dan tujuan yang jelas,"
   ]
  },
  {
   "begin": "126.040",
   "children": [],
   "end": "129.520",
   "id": "f000038",
   "language": "ind",
   "lines": [
    "kita bisa membangun kebiasaan yang mengarah pada kesuksesan."
   ]
  },
  {
   "begin": "129.520",
   "children": [],
   "end": "132.680",
   "id": "f000039",
   "language": "ind",
   "lines": [
    "Setiap orang punya kekuatan untuk menciptakan perubahan,"
   ]
  },
  {
   "begin": "132.680",
   "children": [],
   "end": "135.920",
   "id": "f000040",
   "language": "ind",
   "lines": [
    "dan pagi hari adalah titik awal yang sempurna untuk itu."
   ]
  }
 ]
